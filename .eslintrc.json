{
  "extends": ["next", "next/core-web-vitals"],
  "plugins": ["import"],
  "rules": {
    "no-unused-vars": "error",
    "no-console": "warn"
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    },
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true,
        "project": "./tsconfig.json"
      }
    }
  }
}
